<script>
  console.log('req.payload from navbar: ', JSON.parse(window.localStorage.getItem('user')))
  var user = JSON.parse(window.localStorage.getItem('user'))
  if (user) {
    document.querySelector('#navbarSupportedContent').querySelector('.user-name').innerHTML = user.userName
    document.querySelector('#navbarSupportedContent').querySelector('.avatar-container').querySelector('img').src = user.avatar
  }

  const logout = () => {
    window.localStorage.removeItem('accessToken')
    window.localStorage.removeItem('user')
    window.location.href = '/'
  }
</script>